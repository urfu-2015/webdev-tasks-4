.gallery__carousel__slide {
    display: inline-block
    background: white
    position: absolute
    padding: 10px
    margin: 7px
    box-shadow: 0 0 10px grey
    height: 300px
    vertical-align: top
    transition: left 2s ease-out 0s, top 2s ease-in 0s, transform 1s ease-in-out 0s
    
}

@import "__image/slide__image.styl"